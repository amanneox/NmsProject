    window.addEventListener('load', () => {
      const loader = document.getElementById('loader');
      setTimeout(() => {
        loader.classList.add('fadeOut');
      }, 300);
    });
  </script>
  <div>
  	<% include ../partials/sidebar %>
    <div class="page-container">
        	<% include ../partials/header %>
          <main class="main-content bgc-grey-100">
            <div id="mainContent">
              <div class="container-fluid">
                <h4 class="c-grey-900 mT-10 mB-30">Google Maps</h4>
                <div class="row">
                  <div class="col-md-12">
                    <div class="bgc-white bd bdrs-3 p-20 mB-20">
                      <h6 class="c-grey-900 mB-20">Google Maps</h6>
                      <div id="google-map" style="padding-bottom:75%"></div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </main>
    </div>
  </div>
  <script type="text/javascript" src="./res/js/vendor.js"></script>
  <script type="text/javascript" src="./res/js/bundle.js"></script>
</body>

</html>
